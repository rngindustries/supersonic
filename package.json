{
  "name": "@rngindustries/supersonic",
  "version": "2.1.0",
  "description": "A simple Discord framework for easy prototyping and building of Discord bots.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "dev": "npx ts-node-dev lib/index.ts",
    "build": "npx tsup lib/index.ts --format cjs,esm --clean --dts && npx fix-tsup-cjs"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.13",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.2.3",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "discord.js": "^14.15.3",
    "glob": "^11.0.0",
    "lodash": "^4.17.21"
  }
}
